{
  "name": "BFBAN",
  "version": "2.9.12",
  "private": true,
  "scripts": {
    "serve:development-legacyProvider-windows": "set NODE_OPTIONS=--openssl-legacy-provider && npm run serve:development",
    "serve:development-legacyProvider-macOs": "export NODE_OPTIONS=--openssl-legacy-provider && npm run serve:development",
    "serve:production-legacyProvider-macOs": "export NODE_OPTIONS=--openssl-legacy-provider && npm run serve:production",
    "serve:development": "vue-cli-service serve --mode development",
    "serve:production": "vue-cli-service serve --mode production",
    "serve:staging": "vue-cli-service serve --mode staging",
    "build:development": "vue-cli-service build --dest dist --report --mode development",
    "build:production": "vue-cli-service build --dest dist --mode production",
    "build:staging": "vue-cli-service build --dest dist --mode staging",
    "local-serve": "serve -s front/dist",
    "lint:development": "vue-cli-service lint",
    "lint:development-i18n": "eslint --fix --ext .json  --format node_modules/eslint-plugin-i18n-json/formatter.js src/lang",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/lang/**/*.json\""
  },
